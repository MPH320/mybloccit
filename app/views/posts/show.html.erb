<h1 class="titles"><%= @post.title %></h1>
 <br>
 <div class="row">
   <div class="col-md-8">
     <p><%= @post.body %></p>
     <br>
     <%= link_to "Edit", edit_post_path(@post), class: 'btn btn-warning' %>
     <%= link_to "Delete Post", @post, method: :delete, class: 'btn btn-danger', data: {confirm: 'Are you sure you want to delete this post?'} %>
   </div>
 </div>